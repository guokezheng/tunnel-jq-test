<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tunnel.platform.mapper.dataInfo.SdPushCallMapper">
    
    <resultMap type="SdPushCall" id="SdPushCallResult">
        <result property="id"    column="id"    />
        <result property="reserveData"    column="reserve_data"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectSdPushCallVo">
        select id, reserve_data, create_by, create_time, update_by, update_time from sd_push_call
    </sql>

    <select id="selectSdPushCallList" parameterType="SdPushCall" resultMap="SdPushCallResult">
        <include refid="selectSdPushCallVo"/>
        <where>  
            <if test="createTime != null "> and create_time = #{createTime}</if>
        </where>
    </select>
    
    <select id="selectSdPushCallById" parameterType="Long" resultMap="SdPushCallResult">
        <include refid="selectSdPushCallVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSdPushCall" parameterType="SdPushCall" useGeneratedKeys="true" keyProperty="id">
        insert into sd_push_call
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="reserveData != null">reserve_data,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="reserveData != null">#{reserveData},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateSdPushCall" parameterType="SdPushCall">
        update sd_push_call
        <trim prefix="SET" suffixOverrides=",">
            <if test="reserveData != null">reserve_data = #{reserveData},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSdPushCallById" parameterType="Long">
        delete from sd_push_call where id = #{id}
    </delete>

    <delete id="deleteSdPushCallByIds" parameterType="String">
        delete from sd_push_call where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
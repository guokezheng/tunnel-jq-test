<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tunnel.platform.mapper.intelligentMonitoring.VehicleWhiteListRecordMapper">

    <resultMap type="VehicleWhiteListRecord" id="VehicleWhiteListRecordResult">
        <result property="id"    column="id"    />
        <result property="licensePlateNumber"    column="license_plate_number"    />
        <result property="createTime"    column="create_time"    />
        <result property="status"    column="status"    />
    </resultMap>

    <sql id="selectVehicleWhiteListRecordVo">
        select id, license_plate_number, create_time, status from vehicle_white_list_record
    </sql>

    <select id="selectVehicleWhiteListRecordList" parameterType="VehicleWhiteListRecord" resultMap="VehicleWhiteListRecordResult">
        <include refid="selectVehicleWhiteListRecordVo"/>
        <where>
            <if test="licensePlateNumber != null  and licensePlateNumber != ''"> and license_plate_number = #{licensePlateNumber}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="startTime != null and startTime != '' and endTime != null and endTime != ''"> and create_time between #{startTime} and #{endTime}</if>
        </where>
    </select>

    <select id="selectVehicleWhiteListRecordById" parameterType="Integer" resultMap="VehicleWhiteListRecordResult">
        <include refid="selectVehicleWhiteListRecordVo"/>
        where id = #{id}
    </select>

    <insert id="insertVehicleWhiteListRecord" parameterType="VehicleWhiteListRecord" useGeneratedKeys="true" keyProperty="id">
        insert into vehicle_white_list_record
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="licensePlateNumber != null and licensePlateNumber != ''">license_plate_number,</if>
            <if test="createTime != null">create_time,</if>
            <if test="status != null">status,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="licensePlateNumber != null and licensePlateNumber != ''">#{licensePlateNumber},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="status != null">#{status},</if>
        </trim>
    </insert>

    <update id="updateVehicleWhiteListRecord" parameterType="VehicleWhiteListRecord">
        update vehicle_white_list_record
        <trim prefix="SET" suffixOverrides=",">
            <if test="licensePlateNumber != null and licensePlateNumber != ''">license_plate_number = #{licensePlateNumber},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="status != null">status = #{status},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteVehicleWhiteListRecordById" parameterType="Integer">
        delete from vehicle_white_list_record where id = #{id}
    </delete>

    <delete id="deleteVehicleWhiteListRecordByIds" parameterType="String">
        delete from vehicle_white_list_record where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
